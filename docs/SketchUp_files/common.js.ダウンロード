   border-bottom: 2px solid $col_blue_light;\r\n    padding-bottom: 0px;\r\n  }\r\n\r\n  &#{$invalid-sel} {\r\n    border-bottom: 2px solid $col_red;\r\n    padding-bottom: 0px;\r\n  }\r\n\r\n  &#{$disabled-sel} {\r\n    background-color: $col_gray_0;\r\n  }\r\n}\r\n\r\n// Standard box shadow that is offset to right and down.\r\n@mixin box-shadow-right-down {\r\n  box-shadow: 2px 2px 5px 0px $drop-shadow-color;\r\n}\r\n\r\n// Standard box shadow that is offset to left and down.\r\n@mixin box-shadow-left-down {\r\n  box-shadow: -2px 2px 5px 0px $drop-shadow-color;\r\n}\r\n\r\n// Standard box shadow that is offset down only.\r\n@mixin box-shadow-down {\r\n  box-shadow: 2px 2px 5px 0 $drop-shadow-color;\r\n}\r\n\r\n// Vertically centers an element.\r\n// http://codepen.io/sebastianekstrom/pen/kzEhe\r\n@mixin vertical-align($position: relative) {\r\n  position: $position;\r\n  top: 50%;\r\n  -webkit-transform: translateY(-50%);\r\n  -ms-transform: translateY(-50%);\r\n  transform: translateY(-50%);\r\n}\r\n\r\n// Defines the border we use as a hover highlight effect.\r\n@mixin hover-higlight-borders {\r\n  border: 1px solid black;\r\n}\r\n\r\n// Adds a border transition to be used in conjunction with\r\n// hover-higlight-borders.\r\n@mixin hover-higlight-transition {\r\n  @include transition(border 200ms ease);\r\n}\r\n\r\n// This is a helper for hover-higlight-borders. If adding a hover border will\r\n// increase the overall height of an element (i.e. it doesn't have any borders\r\n// normally), we can use these fake borders that blend in to the background\r\n// color so that adding a hover border won't cause height changes.\r\n@mixin hover-higlight-decoy-borders($color) {\r\n  border: 1px solid $color;\r\n}\r\n\r\n@mixin non-draggable {\r\n  user-drag: none;\r\n  -webkit-user-drag: none;\r\n}\r\n\r\n@mixin default-border {\r\n  border: 1px solid $col_gray_2;\r\n}\r\n\r\n// Replaces Compass input-placeholder.\r\n@mixin input-placeholder {\r\n  &:-moz-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &::-moz-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &:-ms-input-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &::-webkit-input-placeholder {\r\n    @content;\r\n  }\r\n}\r\n\r\n// Replaces Compass opacity.\r\n@mixin opacity($opacity) {\r\n  @if $opacity == 1 {\r\n    filter: unquote('progid:DXImageTransform.Microsoft.Alpha(enabled=false)');\r\n  } @else {\r\n    filter: unquote(\r\n      'progid:DXImageTransform.Microsoft.Alpha(Opacity=#{round($opacity * 100)})'\r\n    );\r\n  }\r\n\r\n  opacity: $opacity;\r\n}\r\n\r\n// Add this to .select-widget whenever you want to show the dropdown arrow.\r\n@mixin select-widget-dropdown-arrow {\r\n  background-image: url(../images/web/wi_chevron_down.svg);\r\n  background-repeat: no-repeat;\r\n  background-position: right;\r\n  background-size: 24px;\r\n}\r\n\r\n@mixin spin {\r\n  animation: spin 2s linear infinite;\r\n}\r\n\r\n// Replaces Compass transform.\r\n// Note that this does not support 3D transforms like the compass rule.\r\n@mixin transform($transform) {\r\n  -moz-transform: $transform;\r\n  -ms-transform: $transform;\r\n  -webkit-transform: $transform;\r\n  transform: $transform;\r\n}\r\n\r\n// Replaces Compass transition.\r\n@mixin transition($transition) {\r\n  @if nth($transition, 1) == transform {\r\n    -moz-transition: #{'-moz-' + $transition};\r\n    -o-transition: #{'-o-' + $transition};\r\n    -webkit-transition: #{'-webkit-' + $transition};\r\n  } @else {\r\n    -moz-transition: $transition;\r\n    -o-transition: $transition;\r\n    -webkit-transition: $transition;\r\n  }\r\n  transition: $transition;\r\n}\r\n\r\n// Replaces Compass user-select.\r\n@mixin user-select($select) {\r\n  -moz-user-select: #{'-moz-' + $select};\r\n  -ms-user-select: $select;\r\n  -webkit-user-select: $select;\r\n  user-select: $select;\r\n}\r\n\r\n@mixin disabled-filter {\r\n  // Universal icon color filter for $col_gray_4 (#90939f)\r\n  // Code generated from https://codepen.io/sosuke/pen/Pjoqqp\r\n  filter: brightness(0) saturate(100%) invert(64%) sepia(19%) saturate(174%)\r\n    hue-rotate(191deg) brightness(89%) contrast(85%);\r\n}\r\n\r\n@mixin hover-filter {\r\n  // Universal icon color filter for $col_blue_electric (#009ad9)\r\n  // Code generated from https://codepen.io/sosuke/pen/Pjoqqp\r\n  filter: brightness(0) saturate(100%) invert(66%) sepia(83%) saturate(5968%)\r\n    hue-rotate(171deg) brightness(97%) contrast(101%);\r\n}\r\n\r\n@mixin blue-filter {\r\n  // Universal icon color filter for $col_blue || $col_trimble_blue (#0063A3)\r\n  // Code generated from https://codepen.io/sosuke/pen/Pjoqqp\r\n  filter: brightness(0) saturate(100%) invert(17%) sepia(42%) saturate(7267%)\r\n    hue-rotate(190deg) brightness(95%) contrast(101%);\r\n}\r\n\r\n@mixin no_scrollbar {\r\n  // Disables / hides a scrollbar in a container where one would appear\r\n  scrollbar-width: none;\r\n  &::-webkit-scrollbar {\r\n    display: none;\r\n  }\r\n}\r\n\r\n@mixin multiline-text-overflow-ellipsis($lines) {\r\n  // Normally webkit prefixes would be of concern but these are actually widely supported.\r\n  // https://caniuse.com/?search=line-clamp\r\n\r\n  display: block; /* Fallback for non-webkit */\r\n  display: -webkit-box;\r\n  line-clamp: $lines;\r\n  -webkit-line-clamp: $lines;\r\n  -webkit-box-orient: vertical;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  word-break: break-word;\r\n  white-space: normal;\r\n}\r\n","/**\r\n * A color palette for the Trimble Modus Design System.\r\n *\r\n * Copyright 2022 Trimble Inc.\r\n */\r\n\r\n// GREEN\r\n$col_green: #1e8a44;\r\n$col_green_light: #4ea646;\r\n$col_green_dark: #006638;\r\n$col_green_pale: #e0eccf;\r\n\r\n// RED\r\n$col_red: #da212c;\r\n$col_red_light: #e86363;\r\n$col_red_dark: #ab1f26;\r\n$col_red_pale: #fbd4d7;\r\n\r\n// YELLOW\r\n$col_yellow: #fbad26;\r\n$col_yellow_light: #fec157;\r\n$col_yellow_dark: #e49325;\r\n$col_yellow_pale: #fff5e4;\r\n\r\n// BLUE\r\n$col_blue: #0063a3;\r\n$col_blue_light: