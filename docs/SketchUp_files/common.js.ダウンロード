on(border 200ms ease);\r\n}\r\n\r\n// This is a helper for hover-higlight-borders. If adding a hover border will\r\n// increase the overall height of an element (i.e. it doesn't have any borders\r\n// normally), we can use these fake borders that blend in to the background\r\n// color so that adding a hover border won't cause height changes.\r\n@mixin hover-higlight-decoy-borders($color) {\r\n  border: 1px solid $color;\r\n}\r\n\r\n@mixin non-draggable {\r\n  user-drag: none;\r\n  -webkit-user-drag: none;\r\n}\r\n\r\n@mixin default-border {\r\n  border: 1px solid $col_gray_2;\r\n}\r\n\r\n// Replaces Compass input-placeholder.\r\n@mixin input-placeholder {\r\n  &:-moz-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &::-moz-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &:-ms-input-placeholder {\r\n    @content;\r\n  }\r\n\r\n  &::-webkit-input-placeholder {\r\n    @content;\r\n  }\r\n}\r\n\r\n// Replaces Compass opacity.\r\n@mixin opacity($opacity) {\r\n  @if $opacity == 1 {\r\n    filter: unquote('progid:DXImageTransform.Microsoft.Alpha(enabled=false)');\r\n  } @else {\r\n    filter: unquote(\r\n      'progid:DXImageTransform.Microsoft.Alpha(Opacity=#{round($opacity * 100)})'\r\n    );\r\n  }\r\n\r\n  opacity: $opacity;\r\n}\r\n\r\n// Add this to .select-widget whenever you want to show the dropdown arrow.\r\n@mixin select-widget-dropdown-arrow {\r\n  background-image: url(../images/web/wi_chevron_down.svg);\r\n  background-repeat: no-repeat;\r\n  background-position: right;\r\n  background-size: 24px;\r\n}\r\n\r\n@mixin spin {\r\n  animation: spin 2s linear infinite;\r\n}\r\n\r\n// Replaces Compass transform.\r\n// Note that this does not support 3D transforms like the compass rule.\r\n@mixin transform($transform) {\r\n  -moz-transform: $transform;\r\n  -ms-transform: $transform;\r\n  -webkit-transform: $transform;\r\n  transform: $transform;\r\n}\r\n\r\n// Replaces Compass transition.\r\n@mixin transition($transition) {\r\n  @if nth($transition, 1) == transform {\r